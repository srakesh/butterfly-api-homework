{
  "extends": ["@mapbox/eslint-config-mapbox", "prettier"],
  "env": {
    "es6": false,
    "commonjs": true
  },
  "rules": {
    "no-var": "off",
    "prefer-const": "off",
    "prefer-arrow-callback": "off"
  },
  "overrides": [
    {
      "files": ["*.test.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
